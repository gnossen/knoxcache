{
  "name": "Knox",
  "description": "Cache web resources.",
  "manifest_version": 3,
  "action": {
    "default_popup": "knox.html"
  },
  "permissions": [
    "tabs"
  ],
  "host_permissions": [
    "http://knox/*"
  ],
  "version": "0.1.0",
  "icons": {
    "16": "logo16.png",
    "32": "logo32.png",
    "64": "logo64.png"
  }
}
