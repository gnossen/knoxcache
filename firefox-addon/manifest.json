{
  "manifest_version": 2,
  "name": "Knox",
  "version": "0.1",

  "description": "Cache web resources.",
  "browser_action": {
    "default_title": "Knox",
    "default_popup": "knox.html"
  },
  "permissions": [
    "tabs"
  ],
  "icons": {
    "16": "logo16.png",
    "32": "logo32.png",
    "64": "logo64.png"
  }
}
